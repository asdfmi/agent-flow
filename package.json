{
  "name": "agent-flow-root",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "workspaces": [
    "domain",
    "portal",
    "runner"
  ],
  "scripts": {
    "lint": "eslint .",
    "format": "prettier --write .",
    "dev:portal": "pnpm --dir portal run dev:api",
    "dev:portal:ui": "pnpm --dir portal run dev:ui",
    "dev:runner": "pnpm --dir runner run dev:runner",
    "dev": "concurrently \"pnpm run dev:portal:ui\" \"pnpm run dev:portal\" \"pnpm run dev:runner\"",
    "prisma:studio": "pnpm --dir portal exec prisma studio --schema prisma/schema.prisma"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "globals": "^16.5.0",
    "prettier": "^3.4.2",
    "concurrently": "^9.1.0",
    "tsx": "^4.19.1"
  },
  "packageManager": "pnpm@10.20.0"
}
